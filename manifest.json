{
  "manifest_version": 2,
  "name": "WebExtensions Capture",
  "description": "WebExtensions Capture",
  "version": "0.1",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [ "<all_urls>" ],
      "js": [ "build/content.js" ],
      "run_at": "document_end",
      "match_about_blank": true
    }
  ],
  "background": {
    "scripts": [
      "build/background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
